ground: linear-gradient(90deg, transparent, rgba(255,255,255,0.3), transparent);\n    transition: left 0.5s ease;\n}\n\n.btn:hover::before {\n    left: 100%;\n}\n\n.btn:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 6px 20px rgba(25, 118, 210, 0.4);\n}\n\n.winter-section {\n    border-left: 4px solid #1976d2;\n}\n\n.newyear-section {\n    border-left: 4px solid #42a5f5;\n}\n\n@media (max-width: 768px) {\n    .level-cards {\n        grid-template-columns: 1fr;\n        gap: 1.5rem;\n        padding: 2rem 0;\n    }\n    \n    .advent-header h2 {\n        font-size: 2rem;\n    }\n    \n    .level-card {\n        padding: 1.5rem;\n    }\n    \n    .level-card h3 {\n        font-size: 1.2rem;\n        padding-right: 3rem;\n    }\n}\n</style>\n\n<div class=\"advent-header\">\n    <h2>‚ùÑÔ∏è New Year Advent Challenge 2025-2026 ‚ùÑÔ∏è</h2>\n    <div class=\"advent-progress\">\n        <div class=\"progress-text\">14 nouveaux d√©fis de programmation pour la nouvelle √©dition</div>\n    </div>\n</div>\n\n<section class=\"level-cards\">\n    <div class=\"level-card winter-section\">\n        <div class=\"card-day-badge\">Jour 1</div>\n        <div class=\"card-content\">\n            <h3>‚ùÑÔ∏è Calculer le prix des courses</h3>\n            <div class=\"card-description\">Ma√Ætrisez les calculs de base pour g√©rer votre budget de f√™tes efficacement.</div>\n            <div class=\"card-difficulty\">‚≠ê Facile</div>\n            <div class=\"level-links\">\n                <a href=\"/premiere/Jour_1/New_Year_Advent\" class=\"btn\">Voir l'exercice</a>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"level-card winter-section\">\n        <div class=\"card-day-badge\">Jour 2</div>\n        <div class=\"card-content\">\n            <h3>‚ùÑÔ∏è Prix par cat√©gorie</h3>\n            <div class=\"card-description\">Structurez vos donn√©es et organisez vos achats par cat√©gories logiques.</div>\n            <div class=\"card-difficulty\">‚≠ê Facile</div